package org.Hangaram.Hangaroid.AndroMoney;

public class CategoryModel {
	String title;
	String IconId;
	boolean bIsIncome = false;
	long lFavorite = 0;
	long ldx = 0;
	
	CategoryModel(String title, String iconId, boolean isIncome, long favorite, long ldx)
	{
		this.title = title;
		this.IconId = iconId;
		this.bIsIncome = isIncome;
		this.lFavorite = favorite;
		this.ldx = ldx;
	}
	
	public String getTitle()
	{
		return this.title;
	}
	
	public boolean getIsIncome()
	{
		return bIsIncome;
	}
	
	public long getFavorite()
	{
		return lFavorite;
	}
	
	public String getIconId()
	{
		return IconId;
	}
	
	public long getldx()
	{
		return ldx;
	}
}
